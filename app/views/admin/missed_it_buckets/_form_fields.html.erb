<%= form_block "Details" do %>
  <%= f.input :title %>
<% end %>

<%= form_block "Content" do %>
  <%= f.association :contents %>
<% end %>

<script type="text/javascript">
    (function($) {
        $.getScript("http://<%=Rails.application.config.scpr.host%>/assets/contentbase_api.js",function() {            
            cbAPI = new scpr.ContentBaseAPI()
            dropzone = cbAPI.buildContentDropZone({el:"#contents-group"{% if original.model_content_type == "content/missed" %},autofill:{bucket:{{ original.id }}}{% endif %}})
        });
    })(django.jQuery);
</script>
