<% page_title "Compare: #{@object.to_title}" %>
<h3><%= @version_a.to_title %> .. <%= @version_b.to_title %></h3>

<%= cache @cache_key do %>
	<% @attribute_diffs.each do |attribute, diff| %>
		<h4><%= attribute.titleize %></h4>
		<div class="well">
			<%= diff.to_s(:html).html_safe %>
		</div>
		<hr />
	<% end %>
<% end %>
