<% page_title model.to_title.pluralize %>
<%= index_header model %>

<%= list_table @records, model do %>
  <thead>
    <tr>
      <%= render "/admin/shared/headers", list: list %>
    </tr>
  </thead>
  <tbody class="list">
    <% @records.each do |record| %>
      <tr class="<%= model.singular_route_key.dasherize %>">
        <td class="column-manage">
          <%= link_to "Edit", record.admin_edit_path, class: "btn" %>
        </td>

        <%= render "/admin/shared/columns", list: list, record: record %>
      </tr>
    <% end %>
  </tbody>
<% end %>
