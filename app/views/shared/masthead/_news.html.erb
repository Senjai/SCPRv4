<div class="inner-container">
  <div class="row-fluid">
    <div class="span6">
      <div class="section first">
        <h6>Categories</h6>
        <nav>
          <div class="row-fluid">
            <div class="span12">
              <ul>
                <li><a href="/local/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Local</a></li>
                <li><a href="/california/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">State</a></li>
                <li><a href="/world/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">U.S./World</a></li>
                <li><a href="/politics/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Politics</a></li>
                <li><a href="/environment/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Eco/Sci</a></li>
              </ul>
            </div>
            <div class="span12">
              <ul>
                <li><a href="/money/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Money</a></li>
                <li><a href="/crime/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Crime &amp; Safety</a></li>
                <li><a href="/education/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Education</a></li>
                <li><a href="/health/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Category" data-ga-label="News">Health</a></li>
              </ul>
            </div>
          </div> <!-- row-fluid -->
          <ul class="content-types pull-down">
            <li><a href="/video/" class="video-icon track-event" data-ga-category="MegaMenu" data-ga-action="Videos" data-ga-label="News">Videos</a></li>
            <li><a href="/blogs/" class="track-event" data-ga-category="MegaMenu" data-ga-action="Blogs" data-ga-label="News">Blogs</a></li>
            <li><a href="/archive/<%= Time.now.yesterday.strftime("%Y/%m/%d") %>/?section=news" class="track-event" data-ga-category="MegaMenu" data-ga-action="Archive" data-ga-label="News">Archive</a></li>
          </ul>
        </nav>
      </div> <!-- section -->
    </div> <!-- span -->
    
    <div class="span8">
      <div class="section">
        <h6>Latest News</h6>
        <% content_cache("mega_news_latest") do %>
          <% register_content("contentbase:new") %>
          <%= render "/shared/masthead/featured", content: get_latest_news.first(2) %>
        <% end %>
      </div> <!-- section -->
    </div> <!-- span -->
    
    <div class="span10">
      <div class="section">
        <h6>From Our Blogs</h6>     
        <% content_cache("mega_news_blogs") do %>
          <% register_content("blogs/entry:new") %>
          <%= render "/shared/masthead/from_our_blogs", content: @latest_blogs_news %>
        <% end %>
      </div> <!-- section -->
    </div> <!-- span -->
  </div> <!-- row fluid -->
</div> <!-- inner container -->
