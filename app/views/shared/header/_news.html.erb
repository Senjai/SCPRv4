<div class="inner-container">
	<div class="row-fluid">
		<div class="span6">
			<div class="section first">
				<h6>Categories</h6>
				<nav>
					<div class="row-fluid">
						<div class="span12">
							<ul>
							    <li><a href="/local/">Local</a></li>
							    <li><a href="/california/">State</a></li>
							    <li><a href="/world/">U.S./World</a></li>
							    <li><a href="/politics/">Politics</a></li>
							    <li><a href="/environment/">Eco/Sci</a></li>
							</ul>
						</div>
						<div class="span12">
							<ul>
							    <li><a href="/money/">Money</a></li>
							    <li><a href="/crime/">Crime &amp; Safety</a></li>
							    <li><a href="/education/">Education</a></li>
							    <li><a href="/health/">Health</a></li>
							</ul>
						</div>
					</div> <!-- row-fluid -->
					<ul class="content-types pull-down">
					    <li><a href="/video/" class="video-icon">Videos</a></li>
					    <li><a href="/blogs/">Blogs</a></li>
						<li><a href="/network/">Network</a></li>
					</ul>
				</nav>
			</div> <!-- section -->
		</div> <!-- span -->
		
		<div class="span8">
			<div class="section">
				<h6>Latest News</h6>
				<% content_cache("mega_news_latest") do %>
					<% register_content("contentbase:new") %>
					<%= render "shared/header/featured", content: get_latest_news.first(2) %>
				<% end %>
			</div> <!-- section -->
		</div> <!-- span -->
		
		<div class="span10">
			<div class="section">
				<h6>From Our Blogs</h6>			
				<% content_cache("mega_news_blogs") do %>
					<% register_content("blogs/entry:new") %>
					<%= render "shared/header/from_our_blogs", content: @g_latest_blogs_news.limit(3) %>
				<% end %>
			</div> <!-- section -->
		</div> <!-- span -->
	</div> <!-- row fluid -->
</div> <!-- inner container -->
