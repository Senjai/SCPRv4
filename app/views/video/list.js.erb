<%# Handles the Modal pop-up %>
<% if any_to_list? @videos %>
  $('.videos-list .list').html("<%= escape_javascript(render partial: 'video_thumb', collection: @videos) %>");
  $('.videos-list .pages').html("<%= "#{params[:page].to_i} of #{@videos.total_pages}" %>");
  $('.arrow.right').attr("data-page", "<%= @videos.current_page + 1 %>");
  $('.arrow.left').attr("data-page", "<%= @videos.current_page - 1 %>");
<% else %>
  $('.videos-list .list').html("<span class='none-to-list'>There are currently no Videos</span>");
<% end %>
