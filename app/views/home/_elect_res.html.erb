<div class="stacked">
  <div class="row-fluid">
    <%= content_cache("home_leads") do %>
      <% register_content("layout/homepage") %>
      <div class="span24">
        <%= render_content @homepage.content[0], "lead8" if @homepage %>
      </div>
  </div>
  <div class="row-fluid">
      <div class="span24 offleads split">
        <%= render_content @homepage.content[1..-1], "offlead_asset" if @homepage %>
      </div>
    <% end %>
  </div>
  <hr />
</div>
<% content_for :conversation_slot do %>
<% end %>

<% content_for :hero_roadblock do %>
<style>
	.broadcast-bar {display:none;}
</style>

<div class="hero-election">
  <div class="container-fluid">
    <div class="row-fluid">
      <header class="span24 election-header">
        <h1>2012 Election Results</h1>
				<a href="#" class="share-link"><span>Share This</span></a>
      </header>
    </div>
		<div class="row-fluid">
			<section class="span24">
				<div class="header-well clearfix">
					<a href="#" class="live-tweet visible-mobile">Read KPCC’s Election Live-Tweets</a>
					<div class="tweets">
						<span class="live-tweet">@KPCC Live-Tweets:</span>
						<ul>
						    <li class="active"><a href="#">@StoltzeFrankly is tweeting from the on air @RepHowardBerman and @BradSherman debate on @AirTalk. If you can't listen in, follow his tweets</a></li>
						    <li><a href="#">We've launched our #MyBallot voter app. Every race, measure and prop in SoCal, tailored to your address: http://kp.cc/U10IvU  #LA</a></li>
						    <li><a href="#">Backers and opponents of Prop 35 agree human trafficking is a problem, but disagree on how to deal with it: http://kp.cc/QonPKG </a></li>
						</ul>
					</div>
				</div>
			</section>
		</div>
    <div class="row-fluid results">
				<!-- PRESIDENTIAL RESULTS -->
        <article class="span8 results-block relative">
          <header class="first">
          	<h2>U.S. President</h2>
						<aside>54% Reporting</aside>
          </header>
					<section class="candidate obama first">
						<h4>Barack Obama (D) <em class="winner">Winner</em></h4>
						<div class="result relative">
							<strong>273</strong>
							<span>Electoral Votes</span>
						</div>
					</section>
					<section class="candidate romney last">
						<h4>Mitt Romney (R)</h4>
						<div class="result relative">
							<strong>178</strong>
							<span>Electoral Votes</span>
						</div>
					</section>
					<header class="first">
          	<h2>30th District</h2>
						<aside>68% Reporting</aside>
          </header>
					<section class="candidate berman first">
						<h4>Howard Berman</h4>
						<div class="result relative">
							<strong>36<b class="percentage">%</b></strong>
						</div>
					</section>
					<section class="candidate sherman last">
						<h4>Brad Sherman <em class="winner">Winner</em></h4>
						<div class="result relative">
							<strong>42<b class="percentage">%</b></strong>
						</div>
					</section>
					<div class="vert-divider"></div>
				</article>
				<!-- PROPOSITION RESULTS -->
        <article class="span8 results-block relative">
          <header>
          	<h2>CA Propositions</h2>
						<aside>68% Reporting</aside>
          </header>
					<!-- Prop 30 -->
					<section class="initiative first">
						<div class="title">
							<h3>Prop 30</h3>
							<p>Governor’s Tax Initiative</p>
						</div>
						<div class="result relative">
							<div class="yes pass">
								<strong>46<b class="percentage">%</b></strong>
								<span>Yes</span>
							</div>
							<div class="no">
								<strong>39<b class="percentage">%</b></strong>
								<span>No</span>
							</div>
							<div class="vert-divider"></div>
						</div>
					</section>
					<!-- Prop 34 -->
					<section class="initiative">
						<div class="title">
							<h3>Prop 34</h3>
							<p>Repeal Death Penalty</p>
						</div>
						<div class="result relative">
							<div class="yes">
								<strong>39<b class="percentage">%</b></strong>
								<span>Yes</span>
							</div>
							<div class="no fail">
								<strong>52<b class="percentage">%</b></strong>
								<span>No</span>
							</div>
							<div class="vert-divider"></div>
						</div>
					</section>
					<!-- Prop 36 -->
					<section class="initiative">
						<div class="title">
							<h3>Prop 36</h3>
							<p>Repeal "Three Strikes"</p>
						</div>
						<div class="result relative">
							<div class="yes pass">
								<strong>49<b class="percentage">%</b></strong>
								<span>Yes</span>
							</div>
							<div class="no">
								<strong>31<b class="percentage">%</b></strong>
								<span>No</span>
							</div>
							<div class="vert-divider"></div>
						</div>
					</section>
					<!-- Prop 38 -->
					<section class="initiative last">
						<div class="title">
							<h3>Prop 38</h3>
							<p>Education Tax Initiative</p>
						</div>
						<div class="result relative">
							<div class="yes pass">
								<strong>49<b class="percentage">%</b></strong>
								<span>Yes</span>
							</div>
							<div class="no">
								<strong>31<b class="percentage">%</b></strong>
								<span>No</span>
							</div>
							<div class="vert-divider"></div>
						</div>
					</section>
					<div class="vert-divider"></div>
				</article>
				<!-- LA COUNTY DA RESULTS -->
    		<article class="span8 results-block">
    			<header class="last">
          	<h2>LA County DA</h2>
						<aside>82% Reporting</aside>
          </header>
					<section class="candidate lacey first">
						<h4>Jackie Lacey <em class="winner">Winner</em></h4>
						<div class="result relative">
							<strong>62<b class="percentage">%</b></strong>
						</div>
					</section>
					<section class="candidate jackson last">
						<h4>Alan Jackson</h4>
						<div class="result relative">
							<strong>30<b class="percentage">%</b></strong>
						</div>
					</section>
					<header class="last">
          	<h2>Local Measures</h2>
						<aside>82% Reporting</aside>
          </header>
					<!-- MEASURE J -->
					<section class="initiative local first">
						<div class="title">
							<h3>Measure J</h3>
							<p>Transportation Tax</p>
						</div>
						<div class="result relative">
							<div class="yes">
								<strong>36<b class="percentage">%</b></strong>
								<span>Yes</span>
							</div>
							<div class="no fail">
								<strong>61<b class="percentage">%</b></strong>
								<span>No</span>
							</div>
							<div class="vert-divider"></div>
						</div>
					</section>
					<section class="initiative last"></section>
    		</article>
			</div>
  </div>
</div>



<% end %>
