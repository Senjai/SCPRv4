<li class="span2 asset" data-asset="<%=@asset.id%>" id="asset-<%=@asset.id%>">
  <div class="thumbnail">
    <img src="<%= @asset.urls.lsquare %>" />
    <h5><%= @asset.title %></h5>
    <small class="muted"><%= @asset.caption %></small>
    <a class="delete btn btn-mini btn-inverse"><i class="icon-white icon-remove"></i></a>
  </div>
</li>
